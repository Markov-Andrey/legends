//===========================================================================
// Trigger: Initialization
//===========================================================================
function Trig_Initialization_Actions takes nothing returns nothing
    set udg_ArthasDenials = 0.00
    set udg_MalganisConversions = 0.00
    set udg_Citizen_Run_Speed = 250.00
    // ---
    call TriggerExecute( gg_trg_Init_Debug )
    call TriggerExecute( gg_trg_Initialize_Heroes )
    call TriggerExecute( gg_trg_Malganis_AI_and_Level )
    call ConditionalTriggerExecute( gg_trg_Int_01_Difficulty )
    call TriggerExecute( gg_trg_Int_02_Player )
    call TriggerExecute( gg_trg_Int_03_Units )
    call TriggerExecute( gg_trg_Int_04_Environment )
    call TriggerExecute( gg_trg_Form_Building_Groups )
    call TriggerExecute( gg_trg_Initialize_Region_Array )
    call TriggerExecute( gg_trg_Initialize_Buildings_To_Attack_Array )
    call TriggerExecute( gg_trg_Start_Dialoge )
    // Start AI
    call TriggerExecute( gg_trg_Starting_Cash )
    // Time of Day
    call SetTimeOfDay( 19.00 )
    // Cinematic Building Invulnerable
    set udg_InvulBuilding_V = gg_unit_ncb3_0820
    call SetUnitInvulnerable( udg_InvulBuilding_V, true )
    // Mal'Ganis Altar Invulnerable
    call SetUnitInvulnerable( gg_unit_uaod_0019, true )
    // Plagued Citizen Buildings
    set udg_PlaguedBuildings = GetUnitsInRectOfPlayer(GetPlayableMapRect(), Player(11))
    // Repel Gates
    call ModifyGateBJ( bj_GATEOPERATION_OPEN, gg_dest_ATg4_0010 )
    call ModifyGateBJ( bj_GATEOPERATION_OPEN, gg_dest_ATg1_2268 )
    // Heroes Square Militia
    call SetUnitColor( gg_unit_h000_0022, PLAYER_COLOR_WHEAT )
    // Map and Camera
    call CameraSetupApplyForPlayer( true, gg_cam_Game_Starts_Here, udg_ThePlayer, 0.00 )
    call CameraSetupApplyForPlayer( true, gg_cam_Game_Starts_Here, udg_ThePlayer2, 0.00 )
    call TriggerSleepAction( 0.01 )
    call ResetToGameCameraForPlayer( udg_ThePlayer, 0 )
    call ResetToGameCameraForPlayer( udg_ThePlayer2, 0 )
    call CameraSetDepthOfFieldScale( 0.00 )
    call CameraSetFocalDistance( 0.00 )
    call TriggerSleepAction( 0.01 )
    // Visibility
    call CreateFogModifierRectBJ( true, udg_ThePlayer, FOG_OF_WAR_VISIBLE, gg_rct_Reveal_Walls_01 )
