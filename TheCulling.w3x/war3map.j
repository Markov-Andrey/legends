                    if ( Trig_Boss_Spawn_Func002Func001Func003Func003Func005C() ) then
                        set udg_Salramm_Ghoul_Count = ( R2I(udg_ArthasDenials) / 8 )
                        call CreateNUnitsAtLoc( 1, 'u002', Player(5), udg_Salramm_Spawn_Point, bj_UNIT_FACING )
