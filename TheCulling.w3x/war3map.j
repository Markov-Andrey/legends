        return false
    endif
    return true
endfunction

function Trig_Warp_to_Blue_Func008C takes nothing returns boolean
    if ( not ( udg_Warp_Chance == 1 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Warp_to_Blue_Actions takes nothing returns nothing
    local effect vfx
    local effect vfx2
    set udg_Gate_Attack_Unit = GetTriggerUnit()
    set udg_Gate_Attack_Point[1] = GetUnitLoc(udg_Gate_Attack_Unit)
    call AddSpecialEffectLocBJ( udg_Gate_Attack_Point[1], "Abilities\\Spells\\Undead\\Darksummoning\\DarkSummonTarget.mdl" )
    set vfx = bj_lastCreatedEffect
    set udg_Warp_Chance = GetRandomInt(1, 2)
    if ( Trig_Warp_to_Blue_Func008C() ) then
        set udg_Gate_Attack_Point[2] = GetRandomLocInRect(gg_rct_Warp_Exit_West)
        call SetUnitPositionLoc( udg_Gate_Attack_Unit, udg_Gate_Attack_Point[2] )
        call AddSpecialEffectLocBJ( udg_Gate_Attack_Point[2], "Abilities\\Spells\\Undead\\Darksummoning\\DarkSummonTarget.mdl" )
        set vfx2 = bj_lastCreatedEffect
        call RemoveLocation (udg_Gate_Attack_Point[2])
    else
        if ( Trig_Warp_to_Blue_Func008Func001C() ) then
            set udg_Gate_Attack_Point[2] = GetRandomLocInRect(gg_rct_Warp_Exit_East)
            call SetUnitPositionLoc( udg_Gate_Attack_Unit, udg_Gate_Attack_Point[2] )
            call AddSpecialEffectLocBJ( udg_Gate_Attack_Point[2], "Abilities\\Spells\\Undead\\Darksummoning\\DarkSummonTarget.mdl" )
            set vfx2 = bj_lastCreatedEffect
            call RemoveLocation (udg_Gate_Attack_Point[2])
        else
        endif
    endif
    call RemoveLocation (udg_Gate_Attack_Point[1])
    call TriggerSleepAction( 2.00 )
    call DestroyEffect(vfx)
    call DestroyEffect(vfx2)
endfunction

