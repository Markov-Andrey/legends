function Trig_Boss_Spawn_Actions takes nothing returns nothing
    if ( Trig_Boss_Spawn_Func002C() ) then
        set udg_Gorger_House_Unit = GetTriggerUnit()
        set udg_Gorger_Spawn_Point = GetUnitLoc(udg_Gorger_House_Unit)
        set udg_Gorger_Killer_Point = GetUnitLoc(GetKillingUnitBJ())
        call CreateNUnitsAtLoc( 1, 'u000', Player(12), udg_Gorger_Spawn_Point, bj_UNIT_FACING )
        call IssuePointOrderLocBJ( GetLastCreatedUnit(), "attack", udg_Gorger_Killer_Point )
        if IsLocationVisibleToPlayer(udg_Gorger_Spawn_Point, GetOwningPlayer(GetKillingUnit())) then
        call SetUserControlForceOff( bj_FORCE_PLAYER[1] )
        call SetUserControlForceOff( bj_FORCE_PLAYER[15] )
        call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUTIN, 2.00, "ReplaceableTextures\\CameraMasks\\DreamFilter_Mask.blp", 0, 0, 0, 0 )
        call PanCameraToTimedLocForPlayer( Player(1), udg_Gorger_Spawn_Point, 1.00 )
        call PanCameraToTimedLocForPlayer( Player(15), udg_Gorger_Spawn_Point, 1.00 )
        endif
        call ConditionalTriggerExecute( gg_trg_Player_Control_Return )
        call ConditionalTriggerExecute( gg_trg_Meathook_Sound )
        set udg_Gorger_60_Int = 1
        set udg_Gorger_90_Int = 1
        call RemoveLocation (udg_Gorger_Killer_Point)
        call RemoveLocation (udg_Gorger_Spawn_Point)
    else
        if ( Trig_Boss_Spawn_Func002Func001C() ) then
            set udg_Boss_Spawn_Chance = GetRandomInt(1, 4)
            set udg_Boss_Spawn_Chance_B = GetRandomInt(1, 4)
            if ( Trig_Boss_Spawn_Func002Func001Func006C() ) then
                set udg_Gorger_House_Unit = GetTriggerUnit()
                set udg_Gorger_Spawn_Point = GetUnitLoc(udg_Gorger_House_Unit)
                set udg_Gorger_Killer_Point = GetUnitLoc(GetKillingUnitBJ())
                call CreateNUnitsAtLoc( 1, 'u000', Player(12), udg_Gorger_Spawn_Point, bj_UNIT_FACING )
                call IssuePointOrderLocBJ( GetLastCreatedUnit(), "attack", udg_Gorger_Killer_Point )
                if ( Trig_Boss_Spawn_Func002Func001Func006Func006C() ) then
                    call SetUnitAbilityLevelSwapped( 'A000', GetLastCreatedUnit(), 2 )
                else
                endif
                if IsLocationVisibleToPlayer(udg_Gorger_Spawn_Point, GetOwningPlayer(GetKillingUnit())) then
                call SetUserControlForceOff( bj_FORCE_PLAYER[1] )
                call CinematicFadeBJ( bj_CINEFADETYPE_FADEOUTIN, 2.00, "ReplaceableTextures\\CameraMasks\\DreamFilter_Mask.blp", 0, 0, 0, 0 )
                call PanCameraToTimedLocForPlayer( Player(1), udg_Gorger_Spawn_Point, 1.00 )
                endif
                call ConditionalTriggerExecute( gg_trg_Player_Control_Return )
                call ConditionalTriggerExecute( gg_trg_Meathook_Sound )
                set udg_Gorger_60_Int = 1
                set udg_Gorger_90_Int = 1
                call RemoveLocation (udg_Gorger_Killer_Point)
                call RemoveLocation (udg_Gorger_Spawn_Point)
            else
            endif
        else
            set udg_Boss_Spawn_Chance = GetRandomInt(1, 4)
            set udg_Boss_Spawn_Chance_B = GetRandomInt(1, 4)
            if ( Trig_Boss_Spawn_Func002Func001Func003C() ) then
                set udg_Boss_Spawn_Chance = GetRandomInt(1, 4)
                set udg_Boss_Spawn_Chance_B = GetRandomInt(1, 4)
                if ( Trig_Boss_Spawn_Func002Func001Func003Func003C() ) then
                    set udg_Salramm_House_Unit = GetTriggerUnit()
                    set udg_Salramm_Spawn_Point = GetUnitLoc(udg_Salramm_House_Unit)
                    set udg_Salramm_Killer_Point = GetUnitLoc(GetKillingUnitBJ())
                    if ( Trig_Boss_Spawn_Func002Func001Func003Func003Func004C() ) then
                        call CreateNUnitsAtLoc( 1, 'u002', Player(5), udg_Salramm_Spawn_Point, bj_UNIT_FACING )
                        call IssuePointOrderLocBJ( GetLastCreatedUnit(), "attack", udg_Salramm_Killer_Point )
                        set udg_Salramm_Ghoul_Count = ( R2I(udg_ArthasDenials) / 10 )
                        call CreateNUnitsAtLoc( udg_Salramm_Ghoul_Count, 'u003', Player(5), udg_Salramm_Spawn_Point, bj_UNIT_FACING )
                        call IssuePointOrderLocBJ( GetLastCreatedUnit(), "attack", udg_Salramm_Killer_Point )
                    else
